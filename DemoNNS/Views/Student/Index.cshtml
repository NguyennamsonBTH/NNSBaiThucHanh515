@model IEnumerable<DemoNNS.Models.Student>
    <p>
        <a class="btn btn-sucess" asp-action="Create">Create a new student</a>
    </p>
    <table class="table table-all">
        <tr>Student ID</tr>
        <tr>Student Name</tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m => item.StudentID)</td>
                <td>@Html.DisplayFor(m => item.StudentName)</td>
            </tr>
        }
    </table>